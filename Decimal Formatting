#include <stdio.h>
#include <math.h>

int main() {
  double num;
  scanf("%lf", &num);

  printf("%.5lf\n", num);
  printf("%.4lf\n", floor(num * 10000 + 0.5) / 10000);
  printf("%.3lf\n", floor(num * 1000 + 0.5) / 1000);
  printf("%.2lf\n", floor(num * 100 + 0.5) / 100);
  printf("%.1lf\n", floor(num * 10 + 0.5) / 10);
  printf("%.0lf\n", floor(num));

  return 0;
}
